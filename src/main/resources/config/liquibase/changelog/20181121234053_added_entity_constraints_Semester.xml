<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Semester.
    -->
    <changeSet id="20181121234053-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="semesters_id"
                                 baseTableName="semester_subject"
                                 constraintName="fk_semester_subject_semesters_id"
                                 referencedColumnNames="id"
                                 referencedTableName="semester"/>
        <addForeignKeyConstraint baseColumnNames="subjects_id"
                                 baseTableName="semester_subject"
                                 constraintName="fk_semester_subject_subjects_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>
        
        <addForeignKeyConstraint baseColumnNames="course_year_id"
                                 baseTableName="semester"
                                 constraintName="fk_semester_course_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course_year"/>

    </changeSet>
</databaseChangeLog>
